# Copy this file to .env and fill in your own credentials.

# OpenAI API Key (Required for the AI agent)
OPENAI_API_KEY="your_OPENAI_API_KEY_here"

# Wallet Key (For Base Sepolia interaction)
WALLET_PRIVATE_KEY="your_wallet_private_key_here"  # Required for blockchain transactions

# Nillion SecretVault Configuration
# Organization Credentials (provided after registering with SecretVault)
SV_ORG_DID="your_organization_DID_here"
SV_PRIVATE_KEY="your_secret_vault_private_key_here"

# SecretVault Node Configuration
# Node 1
SV_NODE1_URL="https://nildb-nx8v.nillion.network"  # Example URL, replace with your assigned node
SV_NODE1_DID="did:nil:testnet:nillion1qfrl8nje3nvwh6cryj63mz2y6gsdptvn07nx8v"  # Example DID, replace with your assigned node DID

# Node 2
SV_NODE2_URL="https://nildb-p3mx.nillion.network"  # Example URL, replace with your assigned node
SV_NODE2_DID="did:nil:testnet:nillion1uak7fgsp69kzfhdd6lfqv69fnzh3lprg2mp3mx"  # Example DID, replace with your assigned node DID

# Node 3
SV_NODE3_URL="https://nildb-rugk.nillion.network"  # Example URL, replace with your assigned node
SV_NODE3_DID="did:nil:testnet:nillion1kfremrp2mryxrynx66etjl8s7wazxc3rssrugk"  # Example DID, replace with your assigned node DID

# Schema IDs - Obtained after creating schemas in Nillion SecretVault
# You will get these IDs after running the schema creation scripts
SCHEMA_ID_SERVICE_LISTING=""  # Leave empty, will be populated after schema creation
SCHEMA_ID_BOOKING=""          # Leave empty, will be populated after schema creation
SCHEMA_ID_FEEDBACK=""         # Leave empty, will be populated after schema creation

# Optional: Telegram Bot Token (only needed for Telegram mode)
TELEGRAM_BOT_TOKEN="your_bot_token_here"

